entity Department{
	departmentName String
}

entity Employee{
	firstName String,
	lastName String,
	email String,
	phoneNumber String,
	hireDAte DateTime,
	salary Long,
	comissionPct Long
}

relationship OneToMany{
	WrongEntity{employee} to Employee{department}
}

service SampleManager {
  Employee assign(Employee employee, WrongEntity wrong)
}